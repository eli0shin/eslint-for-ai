{
  "name": "eslint-for-ai",
  "version": "1.0.0",
  "description": "eslint rules that you would think are useless because any dev would know better than to write code like this but LLMs write stupid code all the time so these rules are for them.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "type": "tsc --noEmit",
    "lint": "eslint .",
    "prerelease-only": "npm run type && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/parser": "^8.46.1",
    "@typescript-eslint/rule-tester": "^8.46.1",
    "@typescript-eslint/utils": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  }
}
